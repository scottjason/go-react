    "lint:fix": "eslint --fix 'client/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'client/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "keywords": [
    "go",
    "react",
    "gin",
